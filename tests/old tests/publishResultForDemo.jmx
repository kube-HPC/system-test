print (outputFilePath);

props.put(&quot;outputFilePath1&quot;,outputFilePath);
String cycle=cycleName+&quot; &quot;+verName;

jira.addTestToCycle (cycle, testName);



if (outputFilePath==&quot;&quot;)
{
	jira.publishResultsToZephyr (cycle, testName, testResult);
}
else
{
	jira.publishResultsToZephyr (cycle, testName, testResult, outputFilePath);
}

print (&quot;done &quot;+testName);
