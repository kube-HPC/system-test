          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="get all times and caluclate" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">

int putResults = Integer.parseInt(vars.get (&quot;duratrionPut_matchNr&quot;));

int getResults = Integer.parseInt(vars.get (&quot;duratrionGet_matchNr&quot;));

print (&quot;*************&quot;);

int putMax=0;
int getMax=0;

for (int i = 0 ; i &lt; putResults ; i++){
	int put = Integer.parseInt(vars.get (&quot;duratrionPut_&quot;+(i+1)));
	if (put &gt; putMax){
		putMax=put;
	}
}

for (i = 0 ; i &lt; getResults ; i++){
	int get=Integer.parseInt(vars.get (&quot;duratrionGet_&quot;+(i+1)));
	if (get &gt; getMax){
		getMax=get;
	}
}

print (putMax);
print (getMax);



</stringProp>
