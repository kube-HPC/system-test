            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">String startTime=&quot;${__time(,)}&quot;;

startTime=startTime+&quot;000&quot;;

String hourAgo= &quot;${__timeShift(,,PT-1H,,)}&quot;;

hourAgo=hourAgo+&quot;000&quot;;
//print (&quot;****************&quot;);
vars.put (&quot;now&quot;,startTime);
vars.put (&quot;hourAgo&quot;, hourAgo);

//print (startTime);
//print (yesterday);</stringProp>
